<p id="notice"><%= notice %></p>
</br>
<h1>You can view your bookings below!</h1>
</br></br>
<table class="center">
  <thead>
    <tr>
      <th><h3>Consultant<h3></th>
      <th><h3>Date<h3></th>
      <th><h3>Status<h3></th>
      <th><h3>Service<h3></th>
      <th colspan="3"></th>
    </tr>
  </thead>
 
  
  <tbody>

    <% @bookings.each do |booking| %>
      <tr>
        <td><%= Profile.find(booking.consultant.id).first_name + " " + Profile.find(booking.consultant.id).last_name %></td>
        <td><%= booking.check_in.strftime("%d/%m%Y %T") %></td>
        <td><%= booking.status %></td>
        <td><%= Service.find(booking.service.id).title %></td>
        <td><%= link_to 'Show', booking %></td>
      </tr>
      <tr>
        <td></td>
        <td></td>
        <td></td>
        <td></td>
        <td></td>
    </tr>
    <% end %>
  </tbody>
</table>
